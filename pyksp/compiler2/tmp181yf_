
import rope.base.project
from rope.base import libutils

myproject = rope.base.project.Project(
    'E:/packages/pyksp')
#myproject.validate('E:/packages/pyksp/pyksp/compiler2/ui_system.py')
myproject.validate()
ropy = libutils.path_to_resource(myproject, 'E:/packages/pyksp/pyksp/compiler2/ui_system.py')

from rope.refactor.extract import ExtractMethod
changes = ExtractMethod(myproject, ropy, 4893, 5778).get_changes('_pack_to_obj', similar=True)
myproject.do(changes)

myproject.close()
