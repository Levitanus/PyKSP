
import rope.base.project
from rope.base import libutils

myproject = rope.base.project.Project(
    'E:/packages/pyksp')
#myproject.validate('E:/packages/pyksp/pyksp/compiler/stack.py')
myproject.validate()
ropy = libutils.path_to_resource(myproject, 'E:/packages/pyksp/pyksp/compiler/stack.py')

from rope.refactor.extract import ExtractMethod
changes = ExtractMethod(myproject, ropy, 2452, 2529).get_changes('__check_idx', similar=True)
myproject.do(changes)

myproject.close()
