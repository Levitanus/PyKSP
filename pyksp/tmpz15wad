
import rope.base.project
from rope.base import libutils

myproject = rope.base.project.Project(
    'E:/packages/pyksp')
#myproject.validate('E:/packages/pyksp/pyksp/abstract.py')
myproject.validate()
ropy = libutils.path_to_resource(myproject, 'E:/packages/pyksp/pyksp/abstract.py')

from rope.refactor.extract import ExtractMethod
changes = ExtractMethod(myproject, ropy, 7499, 7787).get_changes('_check_block_in_queue', similar=True)
myproject.do(changes)

myproject.close()
